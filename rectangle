#include <iostream>
using namespace std;
int main() {
   int length,width;
   cout<<"enter length and width respectively";
   cin>>length>>width;
   int perimeter = 2*(length+width);
   cout<<"perimeter of the rectangle is:"<<perimeter;
   int area = length*width;cout<<endl;
   cout<<"area of the rectangle is:"<<area;

    return 0;
}
